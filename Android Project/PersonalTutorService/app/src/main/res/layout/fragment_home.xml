<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="edu.uta.cse.personaltutorservice.HomeFragment"
    android:background="@mipmap/backgrounddesign5">

    <!-- TODO: Update blank fragment layout -->
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="438dp">


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/editText"
            android:textSize="18dp"
            android:text="Welcome !"
            android:layout_alignBottom="@+id/logOutButton"
            android:layout_alignParentStart="true"
            android:layout_marginStart="24dp" />


        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Log out"
            android:id="@+id/logOutButton"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true" />

        <AutoCompleteTextView
            android:padding="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/autoCompleteTextView"
            android:hint="@string/quick_search"
            android:layout_below="@+id/editText"
            android:drawableRight="@mipmap/searchicon"
            android:layout_alignStart="@+id/editText"
            android:layout_alignEnd="@+id/logOutButton" />






    </RelativeLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="457dp"
        android:orientation="vertical"
        android:weightSum="1">
        <ExpandableListView
            android:layout_width="match_parent"
            android:layout_height="301dp"
            android:id="@+id/exp_list_view"
            android:layout_weight="1.68">

        </ExpandableListView>

    </LinearLayout>

</FrameLayout>
